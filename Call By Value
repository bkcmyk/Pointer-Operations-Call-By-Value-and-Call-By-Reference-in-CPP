#include <iostream>
using namespace std;

int swap(int x, int y)
{
    int temp = x;
    x = y;
    y = temp;
   
    return (x, y);
}

int main()
{
    int a = 10;
    int b = 20;
    cout << "a= " << a;
    cout << "\nb= " << b;
   
    swap(a, b);
    
    cout<<endl<<"\nAfter Swap \n";
    cout<<endl<<"a="<<a;
    cout<<endl<<"b="<<b;
}

/* OUTPUT


a=10
b=20

After Swap 

a=20
b=10
*/
